class Solution {
public:
    int sumFourDivisors(vector<int>& v) {
long long total=0;
     for(int i=0;i<v.size();i++){
        
        int count=0,sum=0;
        for(int j=1;j<=sqrt(v[i]);j++){
            if(v[i]%j==0){
                count++;
                sum+=j;
            

            if(v[i]/j != j){
                count++;
                sum+=v[i]/j;
            }
        }
        }

        if(count==4)
        total+=sum;
     }

     
return total;
    }
};
