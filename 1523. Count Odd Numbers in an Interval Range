Approach 1:

class Solution {
public:
    int countOdds(int low, int high) {
        int count=0;
        for(int i=low;i<=high;i++){
            if(i%2!=0)
            count++;
        }

        return count;
    }
};

Approach 2:
class Solution {
public:
    int countOdds(int low, int high) {
                
    int ans=(high+1)/2 - (low/2);

        return ans;
    }
};
