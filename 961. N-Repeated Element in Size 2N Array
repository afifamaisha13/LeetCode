class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
     sort(nums.begin(),nums.end());
     int n=nums.size();
     int store;
     for(int i=0;i<2*n-1;i++){
        if(nums[i]==nums[i+1]){
            store=nums[i];
            break;
        }
     }  

     return store; 
    }
};
