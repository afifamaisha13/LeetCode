**Approach 1 (100% beats)

class Solution {
public:
    int minOperations(vector<string>& logs) {

        stack<string>s;

        for(int i=0;i<logs.size();i++){
            if( logs[i]=="../"){
               if(!s.empty()) s.pop();
               continue;
            }
            else if(logs[i]=="./") continue;
            else{
                s.push(logs[i]);
            }
        }
        if(s.empty()) return 0;
        return s.size();
        
    }
};



**Approach 2

