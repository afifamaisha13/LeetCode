class Solution {
public:
    bool isValid(string s) {
       stack<char>st;
       for(auto x:s){
        if(x=='['|| x=='('|| x=='{')
        st.push(x);
        else{

            if(st.size()==0)
                return false;

            if(st.top()=='(' && x==')' || st.top()=='{' && x=='}' || st.top()=='[' && x==']')
                st.pop();
            else
                return false;
        }
       }

       if(st.size()==0)
        return true;
        else
        return false;

    }
};
