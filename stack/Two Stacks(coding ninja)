#include <bits/stdc++.h> 
class TwoStack {

public:
public:
    int *arr;
    int size;
    int top1;
    int top2;


    // Initialize TwoStack.
    TwoStack(int s) {
        this->size=s;
        arr=new int[s];
        top1=-1;
        top2=s;    }
    
    // Push in stack 1.
    void push1(int num) {
        if(top2-top1>1){
            top1++;
            arr[top1]=num;
        }
    }

    // Push in stack 2.
    void push2(int num) {
        if(top2-top1>1){
            top2--;
            arr[top2]=num;
        }
    }

    int pop1() {
        if(top1>-1){
        return arr[top1--];
    }
    else
    return -1;
    }

    int pop2() {
        if(top2<size){
        return arr[top2++];
    }
    else
    return -1;
    }
};
