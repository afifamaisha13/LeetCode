#include<bits/stdc++.h>
using namespace std;

class Stack{
    public:
    int *arr;
    int top;
    int size;

    Stack (int size){
        this->size=size;
        arr=new int[size];
        top=-1;
    }

    void push(int element){
        if(size-1!=top){
            top++;
            arr[top]=element;
        }
        else{
            cout<<"Stack overflow"<<endl;
        }
}

    void pop(){
        if(top>-1){
            cout<<"Poped element is:"<<arr[top]<<endl;
            top--;
}
    else{
        cout<<"Stack underflow"<<endl;
    }
}

    int peek(){
        if(top>-1){
            return arr[top];
        }
        else
        return -1;
}

    bool isEmpty(){
        if(top==-1)
        return true;
        else
        return false;
}

};

int main(){
    Stack s(4);
    s.push(2);
    s.push(3);
    s.push(4);
    s.push(5);

    s.pop();

    cout<<s.peek()<<endl;

    cout<<s.isEmpty()<<endl;


    
}
